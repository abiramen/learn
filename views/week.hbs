<h1>{{course}} Week {{week}}</h1>
<p id="week-desc">{{desc}}</p>

<div class="session-item">
    <p>{{text}}</p>
</div>

<div class="session-item">
    <h3>Slides</h3>
    <div class="nav-border"></div>
    <ul>
        {{#classes}}
        <li class="descripted-item">
            <a href="/{{../year}}T{{../term}}/{{class}}"><span class="item-title">{{course}} {{class}}<span
                        class="icon">keyboard_arrow_right</span></span></a>
            <div id="item-desc">with <strong>{{ assist }}</strong></div>
        </li>
        {{/classes}}
    </ul>
</div>

